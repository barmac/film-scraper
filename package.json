{
  "name": "filmweb-scraper",
  "version": "0.1.0",
  "description": "A simple web scraper intended to seek and save Filmweb average movie ratings.",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run lint && tsc",
    "test": "jest --coverage --detectOpenHandles",
    "debug": "npm run build && npm run watch-debug",
    "watch": "npm run watch-ts & nodemon dist/app.js",
    "watch-debug": "npm run watch-ts && nodemon --inspect dist/app.js",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "axios": "0.18.0",
    "cheerio": "1.0.0-rc.2",
    "source-map-support": "0.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/cheerio": "0.22.8",
    "@types/jest": "23.1.3",
    "@types/node": "10.5.1",
    "jest": "23.2.0",
    "nodemon": "1.17.5",
    "serverless-offline": "3.25.6",
    "serverless-webpack": "4.0.0",
    "ts-jest": "22.4.6",
    "ts-loader": "2.3.7",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "webpack": "3.6.0"
  },
  "author": "Maciej Barelkowski",
  "license": "MIT"
}
